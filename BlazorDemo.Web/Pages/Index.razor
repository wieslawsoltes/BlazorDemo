@page "/"

<Dialogs @ref=_dialogs />

<AvaloniaView />

@code {

    private Dialogs? _dialogs;

    protected override void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            Console.WriteLine($"InputDialogService={_dialogs}");
            BlazorDemo.App.Dialogs = _dialogs;
        }
        base.OnAfterRender(firstRender);
    }
}